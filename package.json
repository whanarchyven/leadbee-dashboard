    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.23.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.539.0",
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-animated-numbers": "^1.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
